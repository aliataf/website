// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

model Blog {
  id Int @default(autoincrement()) @id
  title String
  content String
  date String
  minutes Int
  photo String
}

model Project {
  id Int @default(autoincrement()) @id
  title String
  content String
  date String
  minutes Int
}